  },
  "homepage": "https://github.com/sndpgautm/MONIFY#readme",
  "dependencies": {
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "mongoose": "^5.5.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
