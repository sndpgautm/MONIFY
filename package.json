  "bugs": {
    "url": "https://github.com/sndpgautm/MONIFY/issues"
  },
  "homepage": "https://github.com/sndpgautm/MONIFY#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "dotenv": "^7.0.0",
    "express": "^4.18.2",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.0",
    "mongoose-currency": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
